@model Technician
@{
    string title = ViewBag.Action + " Technician";
    ViewBag.Title = title;
}

<h2>@title</h2>

<form asp-action="Edit" method="post">
    <div asp-validation-summary="All" class="text-danger"></div>

    <div class="form-group">
        <label asp-for="Technicianname">Name </label>
        <input asp-for="Technicianname" class="form-control">
    </div>

    <div class="form-group">
        <label asp-for="Technicianemail">Email </label>
        <input type="email" asp-for="Technicianemail" class="form-control">
    </div>

    <div class="form-group">
        <label asp-for="Technicianphone">Phone </label>
        <input maxlength="10" asp-for="Technicianphone" class="form-control">
    </div>

    <input type="hidden" asp-for="TechnicianId" />

    <button type="submit" class="btn btn-primary">@ViewBag.Action</button>
    <a asp-controller="Technician" asp-action="List" class="btn btn-primary">Cancel</a>
</form>